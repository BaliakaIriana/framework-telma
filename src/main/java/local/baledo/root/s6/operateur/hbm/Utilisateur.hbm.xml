<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="local.baledo.root.s6.operateur.model.Utilisateur" table="utilisateur">
        <meta attribute="class-description">
            This class contains the matiere detail.
        </meta>
        <id name="id" type="int" column="idutilisateur">
            <generator class="identity">
            	<!-- <param name="sequence">utilisateur_idutilisateur_seq</param> -->
            </generator>
        </id>
        <property name="nom" column="nom" type="string"/>
        <property name="prenom" column="prenom" type="string"/>
        <property name="cin" column="cin" type="string"/>
    </class>
</hibernate-mapping>